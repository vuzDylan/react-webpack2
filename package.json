{
  "name": "react-webpack2",
  "version": "1.0.0",
  "description": "boilerplate for a webpack2 react project",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=development BABEL_ENV=development node ./server.js",
    "build": "NODE_ENV=production BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "lint": "eslint --ext .js --ext .jsx app",
    "validate": "npm ls"
  },
  "pre-commit": [
    "validate",
    "lint"
  ],
  "author": "Dylan Vuz",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "devDependencies": {
    "abracadabra": "^0.2.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.26.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-inline-chunk-plugin": "^1.1.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.4.5"
  }
}
