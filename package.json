{
  "name": "react-webpack2",
  "version": "1.0.0",
  "description": "boilerplate for a webpack2 react project",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development BABEL_ENV=development node ./server.js",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "lint": "eslint --ext .js --ext .jsx app",
    "validate": "npm ls"
  },
  "pre-push": [
    "validate",
    "lint"
  ],
  "author": "Dylan Vuz",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-sse": "github:rit-sse/eslint",
    "eslint-import-resolver-webpack": "^0.8.2",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.5.3",
    "pre-push": "^0.1.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.5.0"
  }
}
